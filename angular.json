{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "guide": {
      "root": "apps/guide/",
      "sourceRoot": "apps/guide/src",
      "projectType": "application",
      "prefix": "rsi",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/guide",
            "index": "apps/guide/src/index.html",
            "main": "apps/guide/src/main.ts",
            "polyfills": "apps/guide/src/polyfills.ts",
            "tsConfig": "apps/guide/tsconfig.app.json",
            "assets": [
              "apps/guide/src/favicon.ico",
              "apps/guide/src/assets"
            ],
            "styles": [
              "apps/guide/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/guide/src/environments/environment.ts",
                  "with": "apps/guide/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "guide:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "guide:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "guide:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/guide/tsconfig.app.json",
              "apps/guide/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/guide/jest.config.js",
            "tsConfig": "apps/guide/tsconfig.spec.json",
            "setupFile": "apps/guide/src/test-setup.ts"
          }
        }
      }
    },
    "guide-e2e": {
      "root": "apps/guide-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/guide-e2e/protractor.conf.js",
            "devServerTarget": "guide:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "guide:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/guide-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "styles": {
      "root": "libs/styles",
      "sourceRoot": "libs/styles/src",
      "projectType": "library",
      "prefix": "rsi",
      "architect": {
        "build": {
          "builder": "builder coming soon",
          "options": {
            "project": "libs/styles/ng-package.json"
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "prefix": "rsi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/core/tsconfig.lib.json",
            "project": "libs/core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/core/src/test.ts",
            "tsConfig": "libs/core/tsconfig.spec.json",
            "karmaConfig": "libs/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core/tsconfig.lib.json",
              "libs/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "component-header": {
      "root": "libs/component/header",
      "sourceRoot": "libs/component/header/src",
      "projectType": "library",
      "prefix": "rsi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/component/header/tsconfig.lib.json",
            "project": "libs/component/header/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/component/header/tsconfig.lib.json",
              "libs/component/header/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/component/header/jest.config.js",
            "tsConfig": "libs/component/header/tsconfig.spec.json",
            "setupFile": "libs/component/header/src/test-setup.ts"
          }
        }
      }
    },
    "component-confirm-input": {
      "root": "libs/component/confirm-input",
      "sourceRoot": "libs/component/confirm-input/src",
      "projectType": "library",
      "prefix": "rsi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/component/confirm-input/tsconfig.lib.json",
            "project": "libs/component/confirm-input/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/component/confirm-input/tsconfig.lib.json",
              "libs/component/confirm-input/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/component/confirm-input/jest.config.js",
            "tsConfig": "libs/component/confirm-input/tsconfig.spec.json",
            "setupFile": "libs/component/confirm-input/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "yarn"
  },
  "defaultProject": "guide"
}
